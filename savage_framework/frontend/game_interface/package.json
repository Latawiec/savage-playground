{
  "name": "game_interface",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "./dist/**/*",
    "./dist/**/*.js",
    "./dist/**/*.d.ts"
  ],
  "scripts": {
    "postinstall": "npm run proto",
    "proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=../../../savage_communication/proto/game_interface --ts_proto_out=./src/proto ../../../savage_communication/proto/game_interface/*.proto",
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "ts-proto": "^1.181.1"

  },
  "dependencies": {
    "protobufjs": "^7.3.2"
  },
  "peerDependencies": {
    "protobufjs": "^7.3.2"
  },
  "exports": {
    "./proto/*": "./dist/proto/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  }
}
