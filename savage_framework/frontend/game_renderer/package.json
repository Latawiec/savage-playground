{
    "name": "game_renderer",
    "private": true,
    "version": "1.0.0",
    "files": [
        "dist"
    ],
    "main": "./dist/game-renderer-frontend.umd.cjs",
    "module": "./dist/game-renderer-frontend.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/game-renderer-frontend.js",
            "require": "./dist/game-renderer-frontend.umd.cjs",
            "types": "./dist/index.d.ts"
        },
        "./*": "./dist/*"
    },
    "description": "Game Renderer frontend",
    "scripts": {
        "postinstall": "npm run proto",
        "proto": "mkdir -p ./src/.gen/proto && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=../../proto/game_renderer --ts_proto_out=./src/.gen/proto ../../proto/game_renderer/*.proto",
        "dev": "vite",
        "build": "npm run proto && vite build && vue-tsc --emitDeclarationOnly",
        "preview": "vite preview"
    },
    "author": "",
    "devDependencies": {
        "@babel/types": "^7.24.9",
        "@vitejs/plugin-vue": "^5.0.5",
        "@types/node": "^20.14.11",
        "@types/pngjs": "^6.0.5",
        "ts-node": "^10.9.2",
        "ts-proto": "^1.181.1",
        "typescript": "^5.5.3",
        "vite": "^5.3.4",
        "vue-tsc": "^2.0.24"
    },
    "dependencies": {
        "vue": "^3.4.31",
        "@types/unzipper": "^0.10.9",
        "memfs": "^4.9.3",
        "pngjs": "^7.0.0",
        "unzipper": "^0.12.2"
    }
}