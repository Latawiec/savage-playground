{
    "name": "proto-chat-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/proto-chat-frontend.umd.cjs",
    "module": "./dist/proto-chat-frontend.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/proto-chat-frontend.js",
            "require": "./dist/proto-chat-frontend.umd.cjs",
            "types": "./dist/index.d.ts"
        },
        "./*" : "./dist/*"
    },
    "scripts": {
        "postinstall": "npm run proto",

        "dev": "vite",
        "proto": "node ./src/.scripts/proto.js ../proto ./src/.gen/proto",
        "types": "vue-tsc",
        "build": "npm run proto && vite build && vue-tsc --emitDeclarationOnly",
        "preview": "vite preview"
    },
    "dependencies": {
        "bootstrap": "^5.3.3",
        "vue": "^3.4.31",

        "game_interface": "file:/../../../savage_framework/frontend/game_interface"
    },
    "peerDependencies": {
        "vue": "^3.4.31",
        "protobufjs": "^7.3.2"
    },
    "devDependencies": {
        "@babel/types": "^7.24.9",
        "@types/node": "^20.2.5",
        "@vitejs/plugin-vue": "^5.0.5",
        "sass": "^1.77.8",
        "typescript": "^5.2.2",
        "vite": "^5.3.4",
        "vite-plugin-lib-inject-css": "^2.1.1",
        "vue-tsc": "^2.0.24",
        
        "proto_gen": "file:/../../../savage_framework/frontend/proto_gen"
    }
}